{
  "compilerOptions": {
    "target": "ES2022",
    "strictNullChecks": true,
    "sourceMap": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "declaration": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "baseUrl": "./src"
  }
}